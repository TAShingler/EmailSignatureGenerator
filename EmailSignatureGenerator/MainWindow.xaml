    </Window.Resources>

    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="24"
                      ResizeBorderThickness="7"
                      UseAeroCaptionButtons="True"
                      GlassFrameThickness="7" />
    </WindowChrome.WindowChrome>

    <Border x:Name="windowMainBorder"
            BorderBrush="DimGray"
            BorderThickness="1">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="30" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <!--/ Title Bar /-->
            <Grid Grid.Row="0"
                  Background="#FFA0A0A0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="32" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>

                <Viewbox Grid.Column="0"
                         Width="16"
                         Height="16"
                         Child="{StaticResource AppLogo1}" />

                <Menu Grid.Column="1"
                      WindowChrome.IsHitTestVisibleInChrome="True"
                      VerticalAlignment="Center"
                      Background="Transparent"
                      Visibility="Collapsed">
                    <MenuItem Header="File" />
                    <MenuItem Header="Edit" />
                </Menu>
                
                <TextBlock Grid.Column="2"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Left"
                           Text="App Title" />
                <Button x:Name="btnMinimizeWindow"
                        Grid.Column="3"
                        WindowChrome.IsHitTestVisibleInChrome="True"
                        Background="Transparent"
                        Style="{StaticResource TitleBarButtonMinimizeWindowStyle}"
                        Click="btnMinimizeWindow_Click" />
                <Button x:Name="btnCloseApplication"
                        Grid.Column="4"
                        WindowChrome.IsHitTestVisibleInChrome="True"
                        Background="Transparent"
                        Style="{StaticResource TitleBarButtonCloseAppStyle}"
                        Click="btnCloseApplication_Click"/>
            </Grid>

            <!--/ Ribbon /-->
            <TextBlock Grid.Row="1"
                       Height="1"
                       Background="DimGray"
                       Width="auto" />

            <!--/ Body /-->
            <Grid Grid.Row="2"
                  Background="Transparent">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition MinHeight="192"
                                   Height="240"
                                   MaxHeight="240" />
                </Grid.RowDefinitions>

                <!--/ Form for Input /-->
                <Border BorderBrush="DarkGray"
                        BorderThickness="0,0,0,1">
                    <ScrollViewer Padding="8"
                                  VerticalScrollBarVisibility="Auto"
                                  HorizontalScrollBarVisibility="Auto">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <!--/ Left Column /-->
                            <Grid Grid.Column="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="16" />
                                    <ColumnDefinition Width="140" />
                                    <ColumnDefinition Width="211" />
                                    <ColumnDefinition Width="24" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                </Grid.RowDefinitions>

                                <!--/ Left-Side Image /-->
                                <Label x:Name="lblImageSource"
                                       Grid.Row="0"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Logo Image:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxImageSource"
                                         IsEnabled="True"
                                         Grid.Row="0"
                                         Grid.Column="2"
                                         VerticalAlignment="Center"
                                         Height="20" />
                                <Button x:Name="btnImageSource"
                                        IsEnabled="True"
                                        Grid.Row="0"
                                        Grid.Column="3"
                                        Margin="2"
                                        Padding="0"
                                        BorderBrush="#FFABADB3"
                                        HorizontalAlignment="Left"
                                        Width="20"
                                        Click="btnImageSource_Click">
                                    <Viewbox Width="18"
                                             Height="18"
                                             Stretch="None">
                                        <Canvas Width="8"
                                                Height="4">
                                            <Polyline Canvas.Left="0"
                                                      Canvas.Top="0"
                                                      Points="0,4 4,0 8,4"
                                                      Fill="#121212" />
                                        </Canvas>
                                    </Viewbox>
                                </Button>

                                <!--/ Emp. Name /-->
                                <Label x:Name="lblEmployeeName"
                                       Grid.Row="1"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Employee Name:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeName"
                                         IsEnabled="True"
                                         Grid.Row="1"
                                         Grid.Column="2"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Emp. Title /-->
                                <Label x:Name="lblEmployeeTitle"
                                       Grid.Row="2"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Employee Title:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeTitle"
                                         Grid.Row="2"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20"
                                         Text="" />

                                <!--/ Emp. Certs/NMLS/etc. /-->
                                <CheckBox x:Name="checkBoxEmployeeCredentials"
                                          Grid.Row="3"
                                          Grid.Column="0"
                                          IsEnabled="True"
                                          VerticalAlignment="Center"
                                          Width="16"
                                          Height="16" />
                                <Label x:Name="lblEmployeeCredentials"
                                       Grid.Row="3"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="NMLS/Certs/Etc.:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeCredentials"
                                         Grid.Row="3"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Emp. Email /-->
                                <Label x:Name="lblEmployeeEmail"
                                       Grid.Row="4"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Employee Email:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeEmail"
                                         Grid.Row="4"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Phone number /-->
                                <Label x:Name="lblEmployeePhone"
                                       Grid.Row="5"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Branch Phone Number:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeePhone"
                                         Grid.Row="5"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Phone Number Extension /-->
                                <CheckBox x:Name="checkBoxEmployeeExtension"
                                          Grid.Row="6"
                                          Grid.Column="0"
                                          IsEnabled="True"
                                          VerticalAlignment="Center"
                                          Width="16"
                                          Height="16" />
                                <Label x:Name="lblEmployeeExtension"
                                       Grid.Row="6"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Employee Extension"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeExtension"
                                         Grid.Row="6"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Direct Phone Number /-->
                                <CheckBox x:Name="checkBoxEmployeeDirectLine"
                                          Grid.Row="7"
                                          Grid.Column="0"
                                          IsEnabled="True"
                                          VerticalAlignment="Center"
                                          Width="16"
                                          Height="16" />
                                <Label x:Name="lblEmployeeDirectLine"
                                       Grid.Row="7"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Direct Line:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeDirectLine"
                                         Grid.Row="7"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />
                            </Grid>

                            <!--/ Right Column /-->
                            <Grid Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="16" />
                                    <ColumnDefinition Width="140" />
                                    <ColumnDefinition Width="211" />
                                    <ColumnDefinition Width="24" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="16" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="24" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="24" />
                                    <!--<RowDefinition Height="24" />-->
                                </Grid.RowDefinitions>

                                <!--/ Address Line 1 /-->
                                <Label x:Name="lblEmployeeAddressLine1"
                                       Grid.Row="0"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Branch Address Line 1:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeAddressLine1"
                                         Grid.Row="0"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Address Line 2 /-->
                                <Label x:Name="lblEmployeeAddressLine2"
                                       Grid.Row="1"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Branch Address Line 2:"
                                       Padding="5,0" />
                                <TextBox x:Name="textBoxEmployeeAddressLine2"
                                         Grid.Row="1"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Website /-->
                                <Label x:Name="lblWebsite"
                                       Grid.Row="2"
                                       Grid.Column="1"
                                       VerticalAlignment="Center"
                                       Content="Company Website:"
                                       Padding="5 0" />
                                <TextBox x:Name="textBoxWebsite"
                                         Grid.Row="2"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Save Location /-->
                                <Label x:Name="lblSaveToPath"
                                       Grid.Row="4"
                                       Grid.Column="1"
                                       Content="Save to:" />
                                <TextBox x:Name="textBoxSaveToPath"
                                         Grid.Row="4"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20"
                                         IsReadOnly="True" />
                                <Button x:Name="btnChooseSaveLocation"
                                        Grid.Row="4"
                                        Grid.Column="3"
                                        IsEnabled="True"
                                        Padding="0"
                                        Margin="2"
                                        BorderThickness="1"
                                        BorderBrush="#FFABADB3"
                                        ClipToBounds="True"
                                        Click="btnChooseSaveLocation_Click">
                                    <Viewbox Width="18"
                                             Height="18"
                                             Stretch="None" >
                                        <Canvas Width="8"
                                                Height="4">
                                            <Path Data="M 0,4 L 4,0 L 8,4 Z"
                                                  Fill="#121212" />
                                        </Canvas>
                                    </Viewbox>
                                </Button>

                                <!--/ Img Name /-->
                                <Label x:Name="lblFileName"
                                       Grid.Row="5"
                                       Grid.Column="1"
                                       Content="File name:" />
                                <TextBox x:Name="textBoxFileName"
                                         Grid.Row="5"
                                         Grid.Column="2"
                                         IsEnabled="True"
                                         VerticalAlignment="Center"
                                         Height="20" />

                                <!--/ Save Button /-->
                                <Grid Grid.Row="7"
                                      Grid.Column="0"
                                      Grid.ColumnSpan="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="200" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    
                                    <Button x:Name="btnSavePng"
                                            Grid.Column="1"
                                            IsEnabled="True"
                                            Content="Save as PNG"
                                            Click="btnSavePng_Click"
                                            BorderBrush="#FFABADB3" />
                                </Grid>
                            </Grid>
                        </Grid>
                    </ScrollViewer>
                </Border>

                <!--/ Business Card View /-->
                <Grid Grid.Row="1"
                      SnapsToDevicePixels="True">
                    <Grid MinWidth="350"
                          Width="Auto"
                          MaxWidth="650"
                          MinHeight="0"
                          Height="Auto"
                          MaxHeight="240"
                          VerticalAlignment="Center"
                          HorizontalAlignment="Center">
                        <Canvas x:Name="canvasBusinessCardShadow"
                                MinWidth="{Binding MinWidth,
                                                   ElementName=businessCardOuterGrid}"
                                Width="{Binding Width,
                                                ElementName=businessCardOuterGrid}" 
                                MaxWidth="{Binding MaxWidth,
                                                   ElementName=businessCardOuterGrid}"
                                MinHeight="{Binding MinHeight,
                                                    ElementName=businessCardOuterGrid}"
                                Height="{Binding Height,
                                                 ElementName=businessCardOuterGrid}"
                                MaxHeight="{Binding MaxHeight,
                                                    ElementName=businessCardOuterGrid}"
                                HorizontalAlignment="{Binding HorizontalAlignment,
                                                              ElementName=businessCardOuterGrid}" 
                                VerticalAlignment="{Binding VerticalAlignment,
                                                            ElementName=businessCardOuterGrid}">
                            <Rectangle Fill="Gray"
                                       Canvas.Left="2"
                                       Canvas.Top="2"
                                       MinWidth="{Binding MinWidth,
                                                          ElementName=canvasBusinessCardShadow}"
                                       Width="{Binding ActualWidth,
                                                          ElementName=canvasBusinessCardShadow}" 
                                       MaxWidth="{Binding MaxWidth,
                                                          ElementName=canvasBusinessCardShadow}"
                                       MinHeight="{Binding MinHeight,
                                                          ElementName=canvasBusinessCardShadow}"
                                       Height="{Binding ActualHeight,
                                                          ElementName=canvasBusinessCardShadow}"
                                       MaxHeight="{Binding MaxHeight,
                                                          ElementName=canvasBusinessCardShadow}" />
                        </Canvas>
                        
                        <Grid x:Name="businessCardOuterGrid"
                              MinWidth="350"
                              Width="Auto"
                              MaxWidth="650"
                              MinHeight="0"
                              Height="Auto"
                              MaxHeight="240"
                              VerticalAlignment="Stretch"
                              HorizontalAlignment="Stretch"
                              Background="White">
                            <Grid Margin="8"
                                  SnapsToDevicePixels="True">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="96" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                
                                <Image Visibility="{Binding ElementName=imageNonSvgIcon,
                                                            Path=Source,
                                                            Converter={StaticResource SourceToVisibilityConverter}}" 
                                       Grid.Column="0"
                                       MinWidth="24"
                                       Width="Auto"
                                       MaxWidth="96"
                                       MinHeight="24"
                                       Height="Auto"
                                       MaxHeight="96"
                                       x:Name="imageNonSvgIcon"
                                       Source="{Binding ElementName=textBoxImageSource,
                                                        Path=Text,
                                                        Converter={StaticResource StringToPngImageConverter}}" />
                                <svg:SVGImage Visibility="{Binding ElementName=imageSvgIcon,
                                                                   Path=FileSource,
                                                                   Converter={StaticResource SourceToVisibilityAsStringConverter}}" 
                                              Grid.Column="0" 
                                              x:Name="imageSvgIcon"
                                              MinWidth="24"
                                              Width="Auto"
                                              MaxWidth="96"
                                              MinHeight="24"
                                              Height="Auto"
                                              MaxHeight="96"
                                              VerticalContentAlignment="Center" 
                                              HorizontalContentAlignment="Center" 
                                              FileSource="{Binding ElementName=textBoxImageSource,
                                                                   Path=Text,
                                                                   Converter={StaticResource StringToSvgImageConverter}}" />
                                <Rectangle Grid.Column="1"
                                           Width="2"
                                           Fill="{DynamicResource BusinessCardPrimaryColorBrush}"
                                           Margin="12,0,12,0"
                                           SnapsToDevicePixels="True" />
                                <Grid Grid.Column="2"
                                      SnapsToDevicePixels="True"
                                      Margin="0,8">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition MinHeight="12"
                                                       Height="12"
                                                       MaxHeight="12" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition MinHeight="4"
                                                       Height="4"
                                                       MaxHeight="4" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition MinHeight="4"
                                                       Height="4"
                                                       MaxHeight="4" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition MinHeight="4"
                                                       Height="4"
                                                       MaxHeight="4" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition MinWidth="24"
                                                          Width="24"
                                                          MaxWidth="24" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <!--/ Employee Name and Credentials /-->
                                    <StackPanel Grid.Row="0"
                                                Grid.Column="0"
                                                Grid.ColumnSpan="2"
                                                Orientation="Vertical">
                                        <TextBlock Height="24"
                                               Text="{Binding Text,
                                                              ElementName=textBoxEmployeeName}"
                                               FontSize="16.8"
                                               FontWeight="Bold"
                                               FontFamily="Segoe UI"
                                               Foreground="{DynamicResource BusinessCardPrimaryColorBrush}" />
                                        <TextBlock Height="16"
                                               Text="{Binding Text,
                                                              ElementName=textBoxEmployeeTitle}"
                                               FontSize="12"
                                               FontWeight="Normal"
                                               FontFamily="Segoe UI Semibold" />
                                        <TextBlock Height="14"
                                               Text="{Binding Text,
                                                              ElementName=textBoxEmployeeCredentials}" 
                                               FontSize="10"
                                               FontWeight="Normal"
                                               FontFamily="Segoe UI"
                                               Visibility="{Binding IsChecked,
                                                                    Converter={StaticResource BooleanToVisibilityConverter},
                                                                    ElementName=checkBoxEmployeeCredentials}" />
                                    </StackPanel>

                                    <!--/ Employee Email /-->
                                    <Viewbox Grid.Row="2"
                                             Grid.Column="0"
                                             Width="12"
                                             Height="12"
                                             VerticalAlignment="Center"
                                             Child="{StaticResource EmailIcon1}"
                                             SnapsToDevicePixels="True" />
                                    <TextBlock x:Name="textBlockEmployeeEmail"
                                               Grid.Row="2"
                                               Grid.Column="1"
                                               Height="14"
                                               Text="{Binding Text,
                                                              ElementName=textBoxEmployeeEmail}"
                                               VerticalAlignment="Center"
                                               FontFamily="Segoe UI"
                                               FontSize="10"
                                               Margin="0" />

                                    <!--/ Employee Phone /-->
                                    <Viewbox Grid.Row="4"
                                             Grid.Column="0"
                                             Width="12"
                                             Height="12"
                                             VerticalAlignment="Center"
                                             Child="{StaticResource PhoneIcon}"
                                             SnapsToDevicePixels="True" />
                                    <StackPanel Grid.Row="4"
                                                Grid.Column="1"
                                                Orientation="Vertical"
                                                Margin="0">
                                        <TextBlock Height="14"
                                                   VerticalAlignment="Center"
                                                   FontFamily="Segoe UI"
                                                   FontSize="10">
                                            <TextBlock.Text>
                                                <MultiBinding Converter="{StaticResource AppendPhoneExtensionConverter}">
                                                    <Binding ElementName="textBoxEmployeePhone"
                                                             Path="Text" />
                                                    <Binding ElementName="textBoxEmployeeExtension"
                                                             Path="Text" />
                                                    <Binding ElementName="checkBoxEmployeeExtension"
                                                             Path="IsChecked" />
                                                </MultiBinding>
                                            </TextBlock.Text>
                                        </TextBlock>
                                        
                                        <TextBlock Height="14"
                                                   Text="{Binding Text,
                                                                  ElementName=textBoxEmployeeDirectLine}"
                                                   VerticalAlignment="Center"
                                                   FontFamily="Segoe UI"
                                                   FontSize="10"
                                                   Visibility="{Binding IsChecked,
                                                                        Converter={StaticResource BooleanToVisibilityConverter},
                                                                        ElementName=checkBoxEmployeeDirectLine}" />
                                    </StackPanel>

                                    <!--/ Employee Address /-->
                                    <Viewbox Grid.Row="6"
                                             Grid.Column="0"
                                             Width="12"
                                             Height="12"
                                             VerticalAlignment="Center"
                                             Child="{StaticResource MapMarkerIcon}"
                                             SnapsToDevicePixels="True" />
                                    <StackPanel Grid.Row="6"
                                                Grid.Column="1"
                                                Orientation="Vertical"
                                                Margin="0"
                                                SnapsToDevicePixels="False">
                                        <TextBlock Height="14"
                                                   Text="{Binding Text,
                                                                  ElementName=textBoxEmployeeAddressLine1}"
                                                   VerticalAlignment="Center"
                                                   FontFamily="Segoe UI"
                                                   FontSize="10" />
                                        <TextBlock Height="14"
                                                   Text="{Binding Text,
                                                                  ElementName=textBoxEmployeeAddressLine2}"
                                                   VerticalAlignment="Center"
                                                   FontFamily="Segoe UI"
                                                   FontSize="10" />
                                    </StackPanel>

                                    <!--/ Employee Email /-->
                                    <Viewbox Grid.Row="8"
                                             Grid.Column="0"
                                             Width="12"
                                             Height="12"
                                             VerticalAlignment="Center"
                                             Child="{StaticResource WorldWideWebIcon}"
                                             SnapsToDevicePixels="True" />
                                    <TextBlock Grid.Row="8"
                                               Grid.Column="1"
                                               Height="14"
                                               Text="{Binding ElementName=textBoxWebsite,
                                                              Path=Text}" 
                                               VerticalAlignment="Center"
                                               FontFamily="Segoe UI"
                                               FontSize="10"
                                               Margin="0" />
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
    </Border>
</Window>